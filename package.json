{
  "name": "gateway-skeleton",
  "description": "Gateway Skeleton",
  "private": true,
  "version": "0.0.0",
  "main": "index.js",
  "devDependencies": {},
  "scripts": {
    "dev": "nodemon --exec ts-node --inspect=10014 -- src/index.ts",
    "build": "rimraf dist/ && tsc && copyfiles -a package.json './node_modules/**' dist/",
    "build:test": "rimraf dist/ && tsc --project tsconfig.test.json && copyfiles package.json test/fixtures/* dist/",
    "start": "node dist/",
    "lint": "tslint  --project tsconfig.json 'src/**/*.ts'",
    "test": "npm run lint && npm run build:test && nyc ava --verbose --serial dist/test/ && nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "release": "export BABEL_DISABLE_CACHE=1 && release-it",
    "release:dry": "export BABEL_DISABLE_CACHE=1 && release-it --dry-run=true",
    "build:changelog": "conventional-changelog -i CHANGELOG.md -s -p jscs -r 0",
    "post:release": "echo Successfully released $npm_package_version",
    "postinstall": "(./bin/setup-git-hooks && yarn run build) || echo 'Not a git repository'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo9/n9-node-gateway-skeleton.git"
  },
  "keywords": [
    "routing-controller",
    "express",
    "typescript"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://github.com/neo9/n9-node-gateway-skeleton",
  "dependencies": {
    "@neo9/n9-node-conf": "^1.0.6",
    "@neo9/n9-node-log": "^2.4.0",
    "@neo9/n9-node-utils": "^1.5.0",
    "@types/http-proxy-middleware": "^0.19.2",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.106",
    "@types/node": "^11.10.4",
    "@types/request-promise-native": "^1.0.14",
    "@types/route-parser": "^0.1.1",
    "@types/std-mocks": "^1.0.0",
    "@types/url-join": "^4.0.0",
    "ava": "^1.0.0-beta.3",
    "bson": "^4.0.1",
    "conventional-changelog-cli": "^1.3.21",
    "copyfiles": "^2.0.0",
    "glob": "^7.1.2",
    "http-proxy-middleware": "^0.19.1",
    "imperium": "^2.0.1",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.5",
    "n9-node-routing": "^1.0.0",
    "nodemon": "^1.17.3",
    "nyc": "^13.3.0",
    "release-it": "7.4.8",
    "request-promise-native": "^1.0.5",
    "route-parser": "^0.0.5",
    "std-mocks": "^1.0.1",
    "tslint": "^5.9.1",
    "typescript": "^3.3.3333",
    "url-join": "^4.0.0"
  }
}
